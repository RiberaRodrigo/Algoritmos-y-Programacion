# Algoritmos_y_Programacion
//FUNCION
void SumaMatdeMat(telemento M[fil][col], int n)
{
	int i, j, k, l, s;
	cout << endl;
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			s=0;
			for(k=0;k<n;k++)
			{
				for(l=0;l<n;l++)
					{
						cout<<M[i][j].mat[k][l]<<"\t ";
						s = s + M[i][j].mat[k][l];
					}
				cout<<endl;
			}
			cout << endl;
			cout << "\nLa suma es: " << s;
			cout << endl << endl;
		}
		cout<<endl;
	}
	cout<<endl;
}
